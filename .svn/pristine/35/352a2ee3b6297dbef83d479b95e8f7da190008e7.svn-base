package com.keyuan.schoolmap.entify;

import java.util.List;

/**
 * Created by Administrator on 2017/10/19.
 */

public class HomeDetails {
    private List<BannerDetails> advert_items;//轮播图信息
    private List<HotClassDetails> course_info;//热门课程信息
    private PopularGift course_pack;

    public List<BannerDetails> getAdvert_items() {
        return advert_items;
    }

    public void setAdvert_items(List<BannerDetails> advert_items) {
        this.advert_items = advert_items;
    }

    public List<HotClassDetails> getCourse_info() {
        return course_info;
    }

    public void setCourse_info(List<HotClassDetails> course_info) {
        this.course_info = course_info;
    }

    public PopularGift getPopularGift() {
        return course_pack;
    }

    public void setPopularGift(PopularGift popularGift) {
        this.course_pack = popularGift;
    }

    public class PopularGift {
        private String uuid;
        private String title;
        private String show_pics;
        private Float pack_price;
        private Float commission_points;

        public String getUuid() {
            return uuid;
        }

        public void setUuid(String uuid) {
            this.uuid = uuid;
        }

        public String getTitle() {
            return title;
        }

        public void setTitle(String title) {
            this.title = title;
        }

        public String getShow_pics() {
            return show_pics;
        }

        public void setShow_pics(String show_pics) {
            this.show_pics = show_pics;
        }

        public Float getPack_price() {
            return pack_price;
        }

        public void setPack_price(Float pack_price) {
            this.pack_price = pack_price;
        }

        public Float getCommission_points() {
            return commission_points;
        }

        public void setCommission_points(Float commission_points) {
            this.commission_points = commission_points;
        }
    }

    public class HotClassDetails {
        private String uuid;
        private String title;
        private String show_pics;
        private Float course_price;
        private String commission_points;

        public String getUuid() {
            return uuid;
        }

        public void setUuid(String uuid) {
            this.uuid = uuid;
        }

        public String getTitle() {
            return title;
        }

        public void setTitle(String title) {
            this.title = title;
        }

        public String getShow_pics() {
            return show_pics;
        }

        public void setShow_pics(String show_pics) {
            this.show_pics = show_pics;
        }

        public Float getCourse_price() {
            return course_price;
        }

        public void setCourse_price(Float course_price) {
            this.course_price = course_price;
        }

        public String getCommission_points() {
            return commission_points;
        }

        public void setCommission_points(String commission_points) {
            this.commission_points = commission_points;
        }
    }

    public class BannerDetails {
        private String pic;
        private String pic_value_type;
        private String pic_value_parameter;

        public String getPic() {
            return pic;
        }

        public void setPic(String pic) {
            this.pic = pic;
        }

        public String getPic_value_type() {
            return pic_value_type;
        }

        public void setPic_value_type(String pic_value_type) {
            this.pic_value_type = pic_value_type;
        }

        public String getPic_value_parameter() {
            return pic_value_parameter;
        }

        public void setPic_value_parameter(String pic_value_parameter) {
            this.pic_value_parameter = pic_value_parameter;
        }
    }
}
